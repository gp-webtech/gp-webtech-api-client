{
    "name": "gp-web-tech/api-client",
    "description": "Client library for GPWebTech API",
    "license": "MIT",
    "keywords": [
        "api-client"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "docler-labs/api-client-exception": "^1.0 || ^2.0",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.6 || ^2.6",
        "pimple/pimple": "^3.5",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Vanengers\\GpWebtechApiPhpClient\\Generated\\src\\": "src/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "PSR-18 provided by Guzzle 7",
        "php-http/guzzle6-adapter": "PSR-18 wrapper for Guzzle 6"
    }
}
