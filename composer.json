{
    "name": "vanengers/gp-webtech-api-php-client",
    "description": "PHP Client for the gpwebtech API",
    "type": "libary",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Vanengers\\GpWebtechApiPhpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanengers\\GpWebtechApiPhpClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "George van Engers",
            "email": "vanengers@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "7.9.x-dev",
        "vanengers/php-json-object-library": "dev-main",
        "php": "^7.4 || ^8.0",
        "docler-labs/api-client-exception": "^1.0 || ^2.0",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.6 || ^2.6",
        "pimple/pimple": "^3.5",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vanengers/php-openapi-client-generator" : "dev-main"
    }
}
