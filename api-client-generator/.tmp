                "tags": [
                    "Login Check"
                ],
                "summary": "Creates a user token.",
                "description": "Creates a user token.",
                "operationId": "login_check_post",
                "requestBody": {
                    "description": "The login data",
                    "required": true,
                    "content": {
                        "application\/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "nullable": false
                                    },
                                    "password": {
                                        "type": "string",
                                        "nullable": false
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User token created",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "required": [
                                        "token"
                                    ],
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "readOnly": true,
                                            "nullable": false
                                        }
                                    },
                                    "type": "object"
